<% import {HtmlModule} from "@paw/html" %>  
<% apply HtmlModule %>  
<div
        id="{{id}}-inner"
        class="AbField-inner"
>
  <label   class="AbField-label"     [attr.for]="id+'-input'"
  >
    {{config?.fieldLabel}}
  </label>
  <div  class="AbField-input-group"  >
    <div class="AbField-input">
      <div #input class="AbField-input-inner"></div>
    </div>
    <div
            *ngIf="invalid"
            class="AbField-error"
    >
      {{messageError}}
    </div>
  </div>
</div>